@page
@model TicTacToe.Pages.TicTacToe2Model
@{
    ViewData["Title"] = "TicTacToe2";
}

<h2>TicTacToe2</h2>

@for(int i = 0; i < Model.Board.GetLength(0); i++) {
<div class="row">
    @for (int y = 0; y < Model.Board.GetLength(1); y++) {
        <div class="square-2">@Model.Board[i, y]</div>
    }
</div>
}

